contmaior = 0
contmenor = 0

for c in range(1,8):
    ano = int(input('Digite o ano de nascimento da pessoa {}'.format(c)))
    if (2022-ano>=18):
        contmaior = contmaior + 1
    if (2022-ano<18):
        contmenor = contmenor + 1
print('O número de pessoas de maior é {} e o número de menores é {}'.format(contmaior,contmenor))

